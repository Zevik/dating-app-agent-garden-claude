# Firebase Configuration
# קבל את הערכים הללו מ-Firebase Console -> Project Settings -> General -> Your apps
NEXT_PUBLIC_FIREBASE_API_KEY=your-api-key-here
NEXT_PUBLIC_FIREBASE_AUTH_DOMAIN=your-project.firebaseapp.com
NEXT_PUBLIC_FIREBASE_PROJECT_ID=your-project-id
NEXT_PUBLIC_FIREBASE_STORAGE_BUCKET=your-project.appspot.com
NEXT_PUBLIC_FIREBASE_MESSAGING_SENDER_ID=your-sender-id
NEXT_PUBLIC_FIREBASE_APP_ID=your-app-id

# Use Firebase Emulator (true לפיתוח, false לפרודקשן)
NEXT_PUBLIC_USE_EMULATOR=true

# Agent Engine URL
# URL של Agent Engine שלך (לאחר פריסה ב-Agent Garden)
NEXT_PUBLIC_AGENT_ENGINE_URL=https://agent-engine.example.com

# Node Environment
NODE_ENV=development
